{
  "name": "molarity",
  "version": "1.6.0-dev.4",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/molarity.git"
  },
  "devDependencies": {
    "grunt": "~1.4.0"
  },
  "phet": {
    "requirejsNamespace": "MOLARITY",
    "phetLibs": [
      "nitroglycerin"
    ],
    "runnable": true,
    "supportedBrands": [
      "phet",
      "phet-io",
      "adapted-from-phet"
    ],
    "features": {
      "supportsSound": true,
      "supportsInteractiveDescription": true
    },
    "simulation": true
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js"
  }
}